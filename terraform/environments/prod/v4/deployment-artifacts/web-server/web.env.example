# =============================================================================
# Production v4 Web Server Environment Variables
# =============================================================================
# These are build-time environment variables for Next.js
# Must be set during Docker image build (ARG in Dockerfile)
# =============================================================================

# API Server URL (NEXT_PUBLIC_* are exposed to browser)
NEXT_PUBLIC_API_BASE=https://api.sodam.store

# LiveKit Room Configuration
NEXT_PUBLIC_ROOM_NAME=production-room

# Naver Map Client ID
NEXT_PUBLIC_NAVER_MAP_CLIENT_ID=<YOUR_NAVER_MAP_CLIENT_ID>

# OAuth - Kakao
NEXT_PUBLIC_KAKAO_CLIENT_ID=<YOUR_KAKAO_CLIENT_ID>

# OAuth - Google
NEXT_PUBLIC_GOOGLE_CLIENT_ID=<YOUR_GOOGLE_CLIENT_ID>
